{
  "name": "gulp-prepack",
  "version": "0.0.2",
  "description": "A gulp plugin for prepack",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=text ava --verbose --serial"
  },
  "keywords": [
    "gulp",
    "prepack",
    "facebook"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/smildlzj/gulp-prepack"
  },
  "author": "luzhujun",
  "license": "MIT",
  "nyc": {
    "include": [
      "index.js",
      "libs/**/*.js"
    ],
    "instrument": false,
    "reporter": [
      "text-lcov"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false
  },
  "dependencies": {
    "fbjs": "^0.8.16",
    "gulp-util": "^3.0.8",
    "prepack": "^0.2.36",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^4.0.0",
    "nyc": "^10.3.0"
  }
}
